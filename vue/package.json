{
  "name": "web3-avatar-vue",
  "description": "Vue component for generating beautiful Web3 gradient avatars",
  "private": false,
  "version": "1.0.4",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "test": "vitest --environment jsdom --run",
    "build": "vue-tsc && vite build",
    "prepublish": "npm run build && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JackHamer09/web3-avatar.git"
  },
  "keywords": [
    "avatar",
    "web3",
    "vue",
    "gradient",
    "address",
    "generator",
    "ethereum",
    "eth"
  ],
  "author": "JackHamer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JackHamer09/web3-avatar/issues"
  },
  "homepage": "https://web3-avatar.netlify.app/",
  "files": [
    "dist"
  ],
  "main": "./dist/web3-avatar-vue.umd.js",
  "module": "./dist/web3-avatar-vue.es.js",
  "exports": {
    ".": {
      "import": "./dist/web3-avatar-vue.es.js",
      "require": "./dist/web3-avatar-vue.umd.js",
      "types": "./dist/main.d.ts"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "types": "./dist/main.d.ts",
  "dependencies": {
    "vue": "^3.2.45",
    "web3-avatar": "^1.0.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.3.0",
    "jsdom": "^21.1.0",
    "path": "^0.12.7",
    "rollup-plugin-typescript2": "^0.34.1",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^2.0.2",
    "vitest": "^0.29.1",
    "vue-tsc": "^1.0.24"
  }
}
